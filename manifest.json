{
  "manifest_version": 3,
  "name": "Esteban typo3 link",
  "description": "Link to typo3 backend",
  "version": "0.1",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/128.png"
    }
  },
  "icons": {
    "128": "/images/128.png"
  }
}